#!/bin/bash

### A command line radio player with mpv
#
# Usage: radio [a url-part-name] or radio option [arg ...]
#
# radio "regex"
#       - plays a searched item.
#         need quote for "or" regex searching like "wordA\|wordB".
#         don't need quote for "and" regex searching, only white-space needed.
#         ex) radio "sia\|live\|tv" cam  "mango\|full"
#
# radio
#       - plays a random radio, press 'q' to play the next.
#         this loops infinitely, press ctrl-c to exit.
#
# radio {-r|--rotate}
#       - rotates a radio in a specific time.
#         this loops infinitely, press ctrl-c to exit.
#
# radio {-a|--all}
#       - displays all urls, choose a url number.
#
# radio {-g|--group} {en|kr|jp|mv|lt|ln|lc|pl}
#       - plays a group, press 'q' to play the next.
#         this loops randomly and infinitely, press ctrl-c to exit.
#
# radio {-l|--local} ["regex"]
#       - plays local music files.
#         this loops randomly and infinitely, press ctrl-c to exit.
#
# radio {-t|--test}
#       - tests all radios whether work or not.
#         If failed, it logs the file named report.
#
# radio {-e|--edit}
#       - edit this script.
#
# radio {-h|--help}
#       - prints this help.



### TODO
#
# add more urls
# instead quit mpv, can mpv play items continually?



PLAYER="mpv"
RADIOEDITOR="emacsclient"
PS3="Choose : "
LOCALVIDEODIR="/mnt/data/Videos/"
LOCALMUSICDIR="/mnt/data/Music/"
LOCALEXT=".*\.(mp3|flac)$"
RADIOSTARTLINE="### MY RADIO URLS"
RADIOENDLINE="### MY RADIO URLS END"
YOUTUBESTARTLINE="### MY YOUTUBE URLS"
YOUTUBEENDLINE="### MY YOUTUBE URLS END"
LOCALSTARTLINE="### MY LOCAL MUSIC"
LOCALENDLINE="### MY LOCAL MUSIC END"
TESTENDLINE="### TEST END"
SCRIPTDIR=$(dirname -- "$(readlink -f -- "$0")")
REPORTPATH="$SCRIPTDIR/report"
# add date
REPORTFIRSTLINE="### ERROR REPORT"
TESTTIME=10
ROTATIONTIME=600
SKIPPATTERN="^#\?$\|^##\+\|^#[^ ]"

# function_name () {
#    local a=1
#    $1 $2 $3
#    return
# }
#
# function_name $1 $2 $3 ...

x=0
while read -r line ; do
    (( x++ ))
    [[ $line =~ ^($RADIOSTARTLINE) ]] && break
done < "$0"

# printf "\n"
# printf "\n RADIOSTARTLINE's number is $x"
# printf "\n"
urllist=$(tail -n +"$(( x + 4 ))" "$0")
# printf "\n"
# printf "\n $urllist"
# printf "\n"

x=0
while read -r line ; do
    (( x++ ))
    [[ $line =~ ^($RADIOENDLINE) ]] && break
done <<< "$urllist"

radiolist=$(head -n +"$(( x - 2 ))" <<< "$urllist")
# printf "\n"
# printf "\n radiolist is $radiolist"
# printf "\n"
urlname=$(awk -F 'http' '{print $1}' <<< "$urllist")
# printf "\n"
# printf "\n urlname is $urlname"
# printf "\n"
radioname=$(awk -F 'http' '{print $1}' <<< "$radiolist")

# only radio
if [[ -z $1 ]] ; then
    while true ; do
        randomradio=$(shuf -n 1 <<< "$radiolist")
        # SKIPPATTERN for comments
        # basic regex: ‘\?’, ‘\+’, ‘\{’, ‘\|’, ‘\(’, and ‘\)’
        (( $(grep -c "$SKIPPATTERN" <<< "$randomradio") )) && continue
        printf "\n"
        awk -F 'http' '{print $1}' <<< "$randomradio" | \
            sed 's/^# / /' | grep --color=auto .
        printf "\n"
        $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$randomradio") &
        lastbgpid=$!
        wait $lastbgpid
        sleep 1
    done
    exit 0
fi

if [[ $1 =~ ^(-r|--rotate)$ ]] ; then
    while true ; do
        randomradio=$(shuf -n 1 <<< "$radiolist")
        # SKIPPATTERN for comments
        (( $(grep -c "$SKIPPATTERN" <<< "$randomradio") )) && continue
        printf "\n"
        awk -F 'http' '{print $1}' <<< "$randomradio" | \
            sed 's/^# / /' | grep --color=auto .
        printf "\n"
        $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$randomradio") &
        lastbgpid=$!
        sleep $TESTTIME
        # fail playing
        (( ! $(ps -A | grep -c $lastbgpid) )) && continue
        sleep $ROTATIONTIME
        kill $lastbgpid
        sleep 1
    done
    exit 0
fi

[[ $1 =~ ^(-a|--all)$ ]] && radio .

if [[ $1 =~ ^(-g|--group)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        case "$2" in
            en) [[ $line =~ ^(## English Radio) ]] && break ;;
            kr) [[ $line =~ ^(## Korean Radio) ]] && break ;;
            jp) [[ $line =~ ^(## Japanese Radio) ]] && break ;;
            mv) [[ $line =~ ^(## Music Video) ]] && break ;;
            lt) [[ $line =~ ^(## Live Tv) ]] && break ;;
            ln) [[ $line =~ ^(## Live News) ]] && break ;;
            lc) [[ $line =~ ^(## Live Camera) ]] && break ;;
            pl) [[ $line =~ ^(## Play List) ]] && break ;;
            *) printf "\n"
               printf "\n Sorry, there is no \"$2\" group."
               printf "\n Try again."
               printf "\n"
               printf "\n Usage: radio -g {en|kr|jp|mv|lt|ln|lc|pl}"
               printf "\n"
               printf "\n en - English Radio"
               printf "\n kr - Korean Radio"
               printf "\n jp - Japanese Radio"
               printf "\n mv - Music Video"
               printf "\n lt - Live Tv"
               printf "\n ln - Live News"
               printf "\n lc - Live Camera"
               printf "\n pl - Play List"
               printf "\n"
               exit 1
        esac
    done <<< "$urllist"
    grouplist=$(tail -n +"$(( x + 2 ))" <<< "$urllist")
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^##+ ]] && break
    done <<< "$grouplist"
    grouplist=$(head -n "$(( x - 2 ))" <<< "$grouplist")
    while true ; do
        randomradio=$(shuf -n 1 <<< "$grouplist")
        (( $(grep -c "$SKIPPATTERN" <<< "$randomradio") )) && continue
        printf "\n"
        awk -F 'http' '{print $1}' <<< "$randomradio" | \
            sed 's/^# / /' | grep --color=auto .
        printf "\n"
        $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$randomradio") &
        lastbgpid=$!
        wait $lastbgpid
        sleep 1
    done
    exit 0
fi

if [[ $1 =~ ^(-l|--local)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        if [[ $line =~ ^($LOCALSTARTLINE) ]] ; then
            break
        fi
    done <<< "$urllist"
    locallist=$(tail -n +"$(( x + 2 ))" <<< "$urllist")
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^##+ ]] && break
    done <<< "$locallist"
    locallist=$(head -n "$(( x - 2 ))" <<< "$locallist")
    if [[ -z $2 ]] ; then
        while true ; do
            randomlocal=$(shuf -n 1 <<< "$locallist")
            (( $(grep -c "$SKIPPATTERN" <<< "$randomlocal") )) && continue
            printf "\n"
            sed 's/^# / /' <<< "$randomlocal" | grep --color=auto .
            printf "\n"
            randomlocal=$(sed 's/# //' <<< "$randomlocal")
            $PLAYER "$LOCALMUSICDIR""$randomlocal" &
            lastbgpid=$!
            wait $lastbgpid
            sleep 1
        done
    else
        # radio -l "regex"
        cd "$LOCALMUSICDIR"
        shopt -s globstar
        for file in **/* ; do
            if [[ $file =~ $LOCALEXT ]] ; then
                if [[ -z $localoneline ]] ; then
                    localoneline="$file"
                else
                    localoneline="$localoneline":"$file"
                fi
            fi
        done
        locallist=$(sed s/:/\\n/g <<< $localoneline)
        greplocalcount=$(grep -ic "$2" <<< "$locallist")
        if (( $greplocalcount == 1 )) ; then
            thelocalname=$(grep -i "$2" <<< "$locallist")
            printf "\n"
            grep --color=auto . <<< "$thelocalname"
            printf "\n"
            $PLAYER "$LOCALMUSICDIR""$thelocalname"
        elif (( $greplocalcount > 1 )) ; then
            localnames=$(grep -i "$2" <<< "$locallist")
            readarray -t arr_localnames <<< "$localnames"
            # Is it possible to select multiple items like 2,4,6 or 3-7?
            select localfile in "${arr_localnames[@]}" ; do
                if [[ $REPLY =~ ^("*"|"00")$ ]] ; then
                    printf "\n"
                    printf "\n You choose all items."
                    printf "\n They'll be played randomly and infinitely."
                    printf "\n To exit, press Ctrl-c."
                    printf "\n"
                    while true ; do
                        randomlocalname=$(shuf -n 1 <<< "$localnames")
                        printf "\n"
                        grep --color=auto . <<< "$randomlocalname"
                        printf "\n"
                        $PLAYER "$LOCALMUSICDIR""$randomlocalname" &
                        lastbgpid=$!
                        wait $lastbgpid
                        sleep 1
                    done
                    exit 0
                fi
                selectedlocalfile="$localfile"
                break
            done
            printf "\n"
            grep --color=auto . <<< "$selectedlocalfile"
            printf "\n"
            $PLAYER "$LOCALMUSICDIR""$selectedlocalfile"
            exit 0
        else
            printf "\n"
            printf "\n Sorry, the \"$2\" does not match."
            printf "\n Try again."
            printf "\n"
            exit 1
        fi
    fi
    exit 0
fi

if [[ $1 =~ ^(-t|--test)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^($TESTENDLINE) ]] && break
    done < "$urllist"
    testlist=$(head -n +$x "$urllist")
    printf "\n"
    printf "$testlist"
    printf "\n"
    cat > "$REPORTPATH" <<< $REPORTFIRSTLINE
    while read -r line ; do
        (( $(grep -c "$SKIPPATTERN" <<< "$line") )) && continue
        printf "\n"
        awk -F 'http' '{print $1}' <<< "$line" | \
            sed 's/^# / /' | grep --color=auto .
        printf "\n"
        $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$line") &
        lastbgpid=$!
        sleep $TESTTIME
        if (( $(ps aux | grep -c $lastbgpid) == 1 )) ; then
            echo "$line" >> "$REPORTPATH"
        else
            kill $lastbgpid
        fi
        sleep 1
    done <<< "$testlist"
    printf "\n"
    cat "$REPORTPATH"
    printf "\n"
    exit 0
fi

if [[ $1 =~ ^(-e|--edit)$ ]] ; then
    $RADIOEDITOR $HOME/git/radio/radio
    exit 0
fi

if [[ $1 =~ ^(-h|--help)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^(# Usage) ]] && break
    done < "$0"
    helplist=$(tail -n +$(( x - 2 )) "$0")
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ (^$) ]] && break
    done <<< "$helplist"
    helplist=$(head -n $x <<< "$helplist")
    printf "\n"
    sed 's/#\+//' <<< "$helplist"
    printf "\n"
    exit 0
fi

# grep 'and' option
for arg in "$@" ; do
    if [[ -z $grepnames ]] ; then
        grepnames=$(grep -i "$arg" <<< "$urlname")
    else
        grepnames=$(grep -i "$arg" <<< "$grepnames")
    fi
    # printf "\n"
    # printf "\n grepnames is $grepnames"
    # printf "\n"
done
grepnames=$(sed '/^#\?$\|^##\+\|^#[^ ]/d' <<< "$grepnames" | sed 's/^# //')
grepcount=$(grep -ic .  <<< "$grepnames")

# printf "\n"
# printf "\n grepcount is $grepcount"
# printf "\n"

if (( $grepcount == 1 )) ; then
    printf "\n"
    grep --color=auto . <<< "$grepnames"
    printf "\n"
    # grep '-F' for only string like "[live] tv"
    httpurl=$(grep -iF "$grepnames" <<< "$urllist" | \
                  awk -F 'http' '{print FS$2}')
    if [[ $httpurl == "http" ]] ; then
        $PLAYER "$LOCALMUSICDIR"$(grep -iF "$grepnames" <<< "$urllist" | \
                                      sed 's/^# //')
    else
        $PLAYER "$httpurl"
    fi
elif (( $grepcount > 1 )) ; then
    readarray -t arr_grepnames <<< "$grepnames"
    select url in "${arr_grepnames[@]}" ; do
        if [[ $REPLY =~ ^("*"|"00")$ ]] ; then
            printf "\n"
            printf "\n You choose all items."
            printf "\n They'll be played randomly and infinitely."
            printf "\n To exit, press Ctrl-c."
            printf "\n"
            while true ; do
                randomurlname=$(shuf -n 1 <<< "$grepnames")
                printf "\n"
                grep --color=auto . <<< "$randomurlname"
                printf "\n"
                randomurlline=$(grep -iF "$randomurlname" <<< "$grepnames")
                httpurl=$(grep -iF "$randomurlline" <<< "$urllist" | \
                              awk -F 'http' '{print FS$2}')
                if [[ $httpurl == "http" ]] ; then
                    $PLAYER "$LOCALMUSICDIR"$(grep -iF "$randomurlline" <<< \
                                              "$grepnames" | sed 's/^# //')
                else
                    $PLAYER "$httpurl"
                fi
                sleep 1
            done
            exit 0
        fi
        selectedurl="$url"
        break
    done
    printf "\n"
    grep --color=auto . <<< "$selectedurl"
    printf "\n"
    httpurl=$(grep -iF "$selectedurl" <<< "$urllist" | \
                  awk -F 'http' '{print FS$2}')
    if [[ $httpurl == "http" ]] ; then
        $PLAYER "$LOCALMUSICDIR"$(grep -iF "$selectedurl" <<< "$grepnames" | \
                                 sed 's/^# //')
    else
        $PLAYER "$httpurl"
    fi
    exit 0
else
    printf "\n"
    printf "\n Sorry, the \"$1\" does not match."
    printf "\n Try again."
    printf "\n"
    exit 1
fi



