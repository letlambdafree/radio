# this script for my favorite music from various sources
# like radio, youtube, local storage
#
# Usage: radio [urlname] or radio option [arg ...]
#
# radio
#       - plays a shuffled radio, press 'q' to play the next.
#         this loops infinitely, press Ctrl-c repeatedly to exit.
#
# raido "regex"
#       - plays a searched item.
#         need quote for "or" regex searching like "wordA\|wordB".
#         don't need quote for "and" regex searching, only white-space needed.
#         You can use -v to invert match
#         you can select items like 3-5 7 11 15-20
#         ex) radio "sia\|live\|tv" cam "mango\|full"
#             radio sia -v lsd - include "sia" but not "lsd"
# raido @number urlname
#       - you can play random files with @number at the start of search line
#         but can't play a file with a option like --start=03:04 --end=05:33
#         that is, playlist won't be add it
#         ex) radio @100 sia - 100 randomly ordered items will be played
#
# Optinos:
#
# -p | --period
#       - rotates a shuffled radio in a specific time.
#         this loops infinitely, press Ctrl-c repeatedly to exit.
#
# -a | --all
#       - displays all urls, choose a url number.
#
# -g | --group {en|kr|jp|mv|lt|ln|lc|pl}
#       - plays a shuffled radio in a group, press 'q' to play the next.
#         this loops infinitely, press Ctrl-c repeatedly to exit.
#         ex) radio -g jp - play only the Japanese radio randomly
#
# -l | --local ["regex"]
#       - plays a shuffled local music.
#         this loops infinitely, press Ctrl-c repeatedly to exit.
#         ex) radio -l sia  - play only local files with sia
#
# -y | --youtube ["regex"]
#       - not implemented yet
#         search youtube item directly
#
# -t | --test
#       - tests all radios whether work or not.
#         If failed, it logs on the file named report.
#
# -e | --edit
#       - edit this script.
#
# -h | --help
#       - prints this help.



### TODO
#
# add more urls
# add youtube searching directly



# set -u # avoid a undeclared variable

PS3="
# for all, press 00
choose : "
garg0="$0"
garg1="$1"
garg2="$2"
PROGNAME=${garg0##*/}
VERSION="0.1"
PLAYER="mpv"
LOCALVIDEODIR="/mnt/data/Videos/"
LOCALMUSICDIR="/mnt/data/Music/"
LOCALMUSICEXT=".*\.(mp3|flac)$"
RADIOSTARTLINE="### MY RADIO URLS"
RADIOENDLINE="### MY RADIO URLS END"
YOUTUBESTARTLINE="### MY YOUTUBE URLS"
YOUTUBEENDLINE="### MY YOUTUBE URLS END"
LOCALSTARTLINE="### MY LOCAL MUSIC"
LOCALENDLINE="### MY LOCAL MUSIC END"
TESTENDLINE="### TEST END"
SCRIPTDIR="${garg0%/*}"
REPORTPATH="$SCRIPTDIR/report"
# add date
REPORTFIRSTLINE="### ERROR REPORT"
TESTTIME=10
PERIODTIME=600
# basic regex: ‘\?’, ‘\+’, ‘\{’, ‘\|’, ‘\(’, and ‘\)’
SKIPPATTERN="^#\?$\|^##\+\|^#[^ ]\|^#  \+"
playlistnumber=1
# error code for the same item in a row
E_IN_A_ROW=64

test_print() {
    local arg1="$1"
    local arg2="$2"
    local linesnumber
    linesnumber=$(wc -l <<< "$arg2")
    if (( linesnumber == 1 )) ; then
        printf "\n\n${arg1} is ${arg2}\n\n"
    elif (( linesnumber > 1 && linesnumber < 10)) ; then
        printf "\n\n${arg1} is \n${arg2}\n\n"
        printf "\n"
    else
        printf "\n\n${arg1} is \n${arg2}\n\n"
        printf "${linesnumber} lines"
        printf "\n"
    fi
    return 0
}

grep_print() {
    local arg1="$1"
    local webline="https?://"
    local directoryline="/$"
    local localline="/mnt/"
    if [[ "$arg1" =~ "$webline" ]] ; then
        printf "\n"
        awk -F 'http' '{print $1}' <<< "$arg1" \
            | sed 's/^# / /' \
            | grep --color=auto .
        printf "\n"
    elif [[ "$arg1" =~ "$directoryline" ]] ; then
        printf "\n"
        sed 's/^# / /' <<< "$arg1" | grep --color=auto .
        printf "\n"
    elif [[ "$arg1" =~ "$localline" ]] ; then
        printf "\n"
        awk -F '/mnt/' '{print $1}' <<< "$arg1" \
            | sed 's/^# / /' \
            | grep --color=auto .
        printf "\n"
    else
        printf "\n"
        grep --color=auto . <<< "$arg1"
        printf "\n"
    fi
    return 0
}

make_grepnames() {
    # grep 'and' option
    local arg reverse
    for arg in "$@" ; do
        if [[ -z $grepnames ]] ; then
            if [[ $arg =~ ^@[0-9] ]] ; then
                playlistnumber=${arg/@/}
                if (( $playlistnumber < 1 )) ; then
                    printf "\n"
                    printf "\n Sorry, @number is more than 1!"
                    printf "\n Try again."
                    printf "\n"
                    exit 1
                fi
                continue
            fi
            grepnames=$(grep -i "$arg" <<< "$urlname")
        else
            if [[ $arg == -v ]] ; then
                reverse="-v"
                continue
            fi
            grepnames=$(grep -i $reverse "$arg" <<< "$grepnames")
            if [[ -n $reverse ]] ; then
                reverse=""
            fi
        fi
        # test_print grepnames "$grepnames"
    done
}

remove_parts() {
    local arg1="$1"
    if [[ "$arg1" =~ https?:// ]] ; then
        grepnames=$(awk -F 'http' '{print $1}' <<< "$arg1")
    elif [[ "$arg1" =~ /mnt/ ]] ; then
        grepnames=$(awk -F '/mnt/' '{print $1}' <<< "$arg1")
    fi
    return 0
}

shuffle_list() {
    # all items must be selected only once for one-cycle
    local arg1="$1"
    local arg2="$2"
    local firstshuffle
    # eval declare -g $2
    if [[ -z "$shufflelist" ]] ; then
        test_print initial_shufflelist "$arg1"
        firstshuffle=$(shuf -n 1 <<< "$arg1")
        # avoid selecting the same item in a row
        if [[ "$firstshuffle" == "$lastshuffle" ]] ; then
            printf "\n continue \n"
            return $E_IN_A_ROW
        else
            shufflelist=$(grep -vF "$firstshuffle" <<< "$arg1") # needed -F
            arg2="$firstshuffle"
        fi
    else
        test_print shufflelist "$shufflelist"
        lastshuffle=$(shuf -n 1 <<< "$shufflelist")
        shufflelist=$(grep -vF "$lastshuffle" <<< "$shufflelist")
        arg2="$lastshuffle"
    fi
    eval "$2=\$arg2" # bash special form for returning a string
    return 0
}

play() {
    local arg1="$1"
    local partline namepart optionpart
    (( count++ ))
    # test_print count "$count"
    if [[ "$arg1" =~ https?:// ]] ; then
        partline=$(awk -F 'http' '{print FS$2}' <<< "$arg1")
    elif [[ "$arg1" =~ /$ ]] ; then
        partline="$LOCALMUSICDIR"$(sed 's/^# //' <<< "$arg1")
    elif [[ "$arg1" =~ /mnt/ ]] ; then
        partline=$(awk -F '/mnt/' '{print FS$2}' <<< "$arg1")
    else
        printf "\n"
        printf "\n Sorry, something is wrong!"
        printf "\n Try again."
        printf "\n"
        exit 1
    fi
    # playlist with random items as much as playlistnumber
    # >$ mpv item1 item2 item3 item4 item5 ...
    # mpv can't handle item with --option in this case
    if [[ -z "$partlinelist" ]] ; then
        [[ "$partline" =~  --[a-zA-Z]+= ]] || partlinelist="$partline"
    else
        [[ "$partline" =~  --[a-zA-Z]+= ]] \
            || partlinelist=""$partlinelist" "$partline""
    fi
    # test_print partlinelist "$partlinelist"
    (( $count < $playlistnumber )) && return
    if (( $playlistnumber == 1 )) ; then
        # options with --, like --start=00:12 --end=04:16 --volume=120
        if [[ "$partline" =~  --[a-zA-Z]+= ]] ; then
            # test_print partline "$partline"
            namepart=${partline%% --*} # need space for trailing
            # test_print namepart "$namepart"
            optionpart=${partline#"$namepart"}
            $PLAYER "$namepart" $optionpart # don't quote rest options
        else
            $PLAYER "$partline"
        fi
    else
        $PLAYER $partlinelist
    fi
    count=0
    partlinelist=""
    return 0
}

play_radio() {
    local lastbgpid
    if [[ -z "$garg1" ]] ; then
        while true ; do
            radiolist=$(grep -v "$SKIPPATTERN" <<< "$radiolist")
            shuffle_list "$radiolist" randomradio
            (( $? == $E_IN_A_ROW )) && continue
            grep_print "$randomradio"
            $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$randomradio") &
            lastbgpid=$!
            wait $lastbgpid
            sleep 1
        done
    fi
    return 0
}

play_period() {
    if [[ $garg1 =~ ^(-p|--period)$ ]] ; then
        while true ; do
            radiolist=$(grep -v "$SKIPPATTERN" <<< "$radiolist")
            shuffle_list "$radiolist" randomradio
            (( $? == $E_IN_A_ROW )) && continue
            grep_print "$randomradio"
            $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$randomradio") &
            lastbgpid=$!
            sleep $TESTTIME
            # fail playing
            (( ! $(ps -A | grep -c $lastbgpid) )) &&continue
            sleep $PERIODTIME
            kill $lastbgpid
            sleep 1
        done
        return 0
    fi
}

show_all(){
    [[ $garg1 =~ ^(-a|--all)$ ]] && radio .
    return 0
}

play_group() {
    if [[ $garg1 =~ ^(-g|--group)$ ]] ; then
        x=0
        while read -r line ; do
            (( x++ ))
            case "$garg2" in
                en)
                    [[ $line =~ ^(## English Radio) ]] && break ;;
                kr)
                    [[ $line =~ ^(## Korean Radio) ]] && break ;;
                jp)
                    [[ $line =~ ^(## Japanese Radio) ]] && break ;;
                mv)
                    [[ $line =~ ^(## Music Video) ]] && break ;;
                lt)
                    [[ $line =~ ^(## Live TV) ]] && break ;;
                ln)
                    [[ $line =~ ^(## Live News) ]] && break ;;
                lc)
                    [[ $line =~ ^(## Live Camera) ]] && break ;;
                pl)
                    [[ $line =~ ^(## Play List) ]] && break ;;
                *)
                    printf "\n Sorry, there is no \"$2\" group."
                    printf "\n Try again."
                    printf "\n"
                    printf "\n Usage: radio -g {en|kr|jp|mv|lt|ln|lc|pl}"
                    printf "\n"
                    printf "\n en - English Radio"
                    printf "\n kr - Korean Radio"
                    printf "\n jp - Japanese Radio"
                    printf "\n mv - Music Video"
                    printf "\n lt - Live TV"
                    printf "\n ln - Live News"
                    printf "\n lc - Live Camera"
                    printf "\n pl - Play List"
                    printf "\n"
                    exit 2
            esac
        done <<< "$urllist"
        grouplist=$(tail -n +"$(( x + 2 ))" <<< "$urllist")
        x=0
        while read -r line ; do
            (( x++ ))
            [[ $line =~ ^##+ ]] && break
        done <<< "$grouplist"
        grouplist=$(head -n "$(( x - 2 ))" <<< "$grouplist")
        while true ; do
            grouplist=$(grep -v "$SKIPPATTERN" <<< "$grouplist")
            shuffle_list "$grouplist" randomradio
            (( $? == $E_IN_A_ROW )) && continue
            grep_print "$randomradio"
            play "$randomradio"
            sleep 1
        done
        return 0
    fi
}

play_local() {
    if [[ $1 =~ ^(-l|--local)$ ]] ; then
        x=0
        while read -r line ; do
            (( x++ ))
            if [[ $line =~ ^($LOCALSTARTLINE) ]] ; then
                break
            fi
        done <<< "$urllist"
        locallist=$(tail -n +"$(( x + 2 ))" <<< "$urllist")
        x=0
        while read -r line ; do
            (( x++ ))
            [[ $line =~ ^##+ ]] && break
        done <<< "$locallist"
        locallist=$(head -n "$(( x - 2 ))" <<< "$locallist")
        if [[ -z $2 ]] ; then
            while true ; do
                locallist=$(grep -v "$SKIPPATTERN" <<< "$locallist")
                shuffle_list "$locallist" randomlocal
                (( $? == $E_IN_A_ROW )) && continue
                grep_print "$randomlocal"
                randomlocal=$(sed 's/# //' <<< "$randomlocal")
                $PLAYER "$LOCALMUSICDIR""$randomlocal" &
                lastbgpid=$!
                wait $lastbgpid
                sleep 1
            done
        else
            # radio -l "regex"
            cd "$LOCALMUSICDIR"
            shopt -s globstar
            for file in **/* ; do
                if [[ $file =~ $LOCALMUSICEXT ]] ; then
                    if [[ -z $localoneline ]] ; then
                        localoneline="$file"
                    else
                        localoneline="$localoneline":"$file"
                    fi
                fi
            done
            locallist=$(sed s/:/\\n/g <<< $localoneline)
            greplocalcount=$(grep -ic "$2" <<< "$locallist")
            if (( $greplocalcount == 1 )) ; then
                thelocalname=$(grep -i "$2" <<< "$locallist")
                grep_print "$thelocalname"
                $PLAYER "$LOCALMUSICDIR""$thelocalname"
            elif (( $greplocalcount > 1 )) ; then
                localnames=$(grep -i "$2" <<< "$locallist")
                readarray -t arr_localnames <<< "$localnames"
                # Is it possible to select multiple items like 2,4,6 or 3-7?
                select localfile in "${arr_localnames[@]}" ; do
                    if [[ $REPLY =~ ^00$ ]] ; then
                        printf "\n"
                        printf "\n You choose all items."
                        printf "\n They'll be played randomly and infinitely."
                        printf "\n To exit, press Ctrl-c repeatedly."
                        printf "\n"
                        while true ; do
                            shuffle_list "$localnames" randomlocal
                            (( $? == $E_IN_A_ROW )) && continue
                            grep_print "$randomlocal"
                            $PLAYER "$LOCALMUSICDIR""$randomlocal" &
                            lastbgpid=$!
                            wait $lastbgpid
                            sleep 1
                        done
                        exit 0
                    fi
                    selectedlocalfile="$localfile"
                    break
                done
                grep_print "$selectedlocalfile"
                $PLAYER "$LOCALMUSICDIR""$selectedlocalfile"
                exit 0
            else
                printf "\n"
                printf "\n Sorry, the \"$2\" does not match."
                printf "\n Try again."
                printf "\n"
                exit 1
            fi
        fi
        return 0
    fi
}




x=0
while read -r line ; do
    (( x++ ))
    [[ $line =~ ^($RADIOSTARTLINE) ]] && break
done < "$0"
# test_print "RADIOSTARTLINE's number" "$x"
urllist=$(tail -n +"$(( x + 4 ))" "$0")
# test_print urllist "$urllist"

x=0
while read -r line ; do
    (( x++ ))
    [[ $line =~ ^($RADIOENDLINE) ]] && break
done <<< "$urllist"

radiolist=$(head -n +"$(( x - 2 ))" <<< "$urllist")
# test_print radiolist "$radiolist"
urlname=$(awk -F 'http' '{print $1}' <<< "$urllist")
# test_print urlname "$urlname"
radioname=$(awk -F 'http' '{print $1}' <<< "$radiolist")

# only radio
play_radio
play_period
show_all
play_group
play_local




if [[ $1 =~ ^(-t|--test)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^($TESTENDLINE) ]] && break
    done <<< "$urllist"
    testlist=$(head -n +$x <<< "$urllist")
    cat > "$REPORTPATH" <<< $REPORTFIRSTLINE
    while read -r line ; do
        (( $(grep -c "$SKIPPATTERN" <<< "$line") )) &&continue
        grep_print "$line"
        $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$line") &
        lastbgpid=$!
        sleep $TESTTIME
        if (( $(ps aux | grep -c $lastbgpid) == 1 )) ; then
            echo "$line" >> "$REPORTPATH"
        else
            kill $lastbgpid
        fi
        sleep 1
    done <<< "$testlist"
    printf "\n"
    cat "$REPORTPATH"
    printf "\n"
    exit 0
fi

if [[ $1 =~ ^(-e|--edit)$ ]] ; then
    $EDITOR $HOME/git/radio/radio
    exit 0
fi

if [[ $1 =~ ^(-h|--help)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^(# Usage) ]] && break
    done < "$0"
    helplist=$(tail -n +$(( x - 3 )) "$0")
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ (^$) ]] && break
    done <<< "$helplist"
    helplist=$(head -n $x <<< "$helplist")
    printf "\n"
    sed 's/#\+//' <<< "$helplist"
    printf "\n"
    exit 0
fi
make_grepnames
grepnames=$(grep -v "/^#\?$\|^##\+\|^#[^ ]\|^#  \+" <<< "$grepnames" \
                | sed 's/^# //')
grepcount=$(grep -ic .  <<< "$grepnames")
# test_print grepcount "$grepcount"

if (( $grepcount == 1 )) ; then
    grep_print "$grepnames"
    # grep '-F' for only string like "[live] tv"
    grepline=$(grep -iF "$grepnames" <<< "$urllist")
    play "$grepline"
elif (( $grepcount > 1 )) ; then
    remove_parts "$grepnames"
    readarray -t arr_grepnames <<< "$grepnames"
    select url in "${arr_grepnames[@]}" ; do
        if [[ $REPLY =~ ^00$ ]] ; then
            printf "\n"
            printf "\n You choose all items."
            printf "\n They'll be played randomly and infinitely."
            printf "\n To exit, press Ctrl-c repeatedly"
            printf "\n"
            while true ; do
                shuffle_list "$grepnames" randomurlname
                (( $? == $E_IN_A_ROW )) && continue
                grep_print "$randomurlname"
                grepline=$(grep -iF "$randomurlname" <<< "$urllist")
                play "$grepline"
                # sleep 1
            done
            exit 0
        elif [[ $REPLY =~ [0-9]+[-\ ][0-9]+ ]] ; then
            printf "\n"
            printf "\n You select multiple items with number."
            printf "\n they'll be played randomly and infinitely."
            printf "\n To exit, press Ctrl-c repeatedly"
            printf "\n"
            # like 3-5 7 11 15-20 --> 3,5p;11;15,20p for sed
            REPLY=$(sed 's/ /p;/g' <<< "$REPLY" \
                        | sed 's/$/p;/g' \
                        | sed 's/-/,/g')
            grepnames=$(sed -n "$REPLY" <<< "$grepnames")
            while true ; do
                shuffle_list "$grepnames" randomurlname
                (( $? == $E_IN_A_ROW )) && continue
                grep_print "$randomurlname"
                grepline=$(grep -iF "$randomurlname" <<< "$urllist")
                play "$grepline"
                # sleep 1
            done
            exit 0
        elif [[ $REPLY =~ [^0-9] ]] ; then
            # like sia, (1986)
            printf "\n"
            printf "\n You select items with non numbers."
            printf "\n they'll be played randomly and infinitely."
            printf "\n To exit, press Ctrl-c repeatedly"
            printf "\n"
            while true ; do
                grepnames=$(grep -i "$REPLY" <<< "$grepnames")
                shuffle_list "$grepnames" randomurlname
                (( $? == $E_IN_A_ROW )) && continue
                grep_print "$randomurlname"
                grepline=$(grep -iF "$randomurlname" <<< "$urllist")
                play "$grepline"
                # sleep 1
            done
            exit 0
        fi
        selectedurl="$url"
        break
    done
    grep_print "$selectedurl"
    grepline=$(grep -iF "$selectedurl" <<< "$urllist")
    play "$grepline"
    exit 0
else
    printf "\n"
    printf "\n Sorry, the \"$1\" does not match."
    printf "\n Try again."
    printf "\n"
    exit 1
fi
return 0

