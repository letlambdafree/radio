#!/bin/bash

# this script for my favorite music from various sources
# like radio, youtube, local storage
#
# Usage: radio [a url-part-name] or radio option [arg ...]
#
# radio "regex"
#       - plays a searched item.
#         need quote for "or" regex searching like "wordA\|wordB".
#         don't need quote for "and" regex searching, only white-space needed.
#         You can use -v to invert match
#         you can play random files with @number at the start of search line
#         but it can't play files with a option like --start=03:04 --end=05:33
#         ex) radio "sia\|live\|tv" cam "mango\|full"
#             radio sia -v lsd - include "sia" but "lsd"
#             radio @100 sia - 100 random ordered items will be played
#
# radio
#       - plays a random radio, press 'q' to play the next.
#         this loops infinitely, press Ctrl-c repeatedly to exit.
#
# radio {-p|--period}
#       - rotates a radio in a specific time.
#         this loops infinitely, press Ctrl-c repeatedly to exit.
#
# radio {-a|--all}
#       - displays all urls, choose a url number.
#
# radio {-g|--group} {en|kr|jp|mv|lt|ln|lc|pl}
#       - plays a group, press 'q' to play the next.
#         this loops randomly and infinitely, press Ctrl-c repeatedly to exit.
#
# radio {-l|--local} ["regex"]
#       - plays local music files.
#         this loops randomly and infinitely, press Ctrl-c repeatedly to exit.
#
# radio {-t|--test}
#       - tests all radios whether work or not.
#         If failed, it logs on the file named report.
#
# radio {-e|--edit}
#       - edit this script.
#
# radio {-h|--help}
#       - prints this help.



### TODO
#
#  Add more urls
#  Change a random util instead of shuf (not good)
#  Add playlist support



PLAYER="mpv"
PS3="
# for all, press * or 00
choose : "
LOCALVIDEODIR="/mnt/data/Videos/"
LOCALMUSICDIR="/mnt/data/Music/"
LOCALMUSICEXT=".*\.(mp3|flac)$"
RADIOSTARTLINE="### MY RADIO URLS"
RADIOENDLINE="### MY RADIO URLS END"
YOUTUBESTARTLINE="### MY YOUTUBE URLS"
YOUTUBEENDLINE="### MY YOUTUBE URLS END"
LOCALSTARTLINE="### MY LOCAL MUSIC"
LOCALENDLINE="### MY LOCAL MUSIC END"
TESTENDLINE="### TEST END"
SCRIPTDIR=$(dirname -- "$(readlink -f -- "$0")")
REPORTPATH="$SCRIPTDIR/report"
# add date
REPORTFIRSTLINE="### ERROR REPORT"
TESTTIME=10
PERIODTIME=600
SKIPPATTERN="^#\?$\|^##\+\|^#[^ ]\|^#  \+"
playlistnumber=1



play() {
    local arg arg0 arg1 arg2 arg3
    (( count++ ))
    # test-print count "$count"
    if [[ $1 =~ https?:// ]] ; then
        arg=$(awk -F 'http' '{print FS$2}' <<< $1)
    elif [[ $1 =~ /$ ]] ; then
        arg="$LOCALMUSICDIR"$(sed 's/^# //' <<< $1)
    elif [[ $1 =~ /mnt/ ]] ; then
        arg=$(awk -F '/mnt/' '{print FS$2}' <<< $1)
    else
        printf "\n"
        printf "\n Sorry, something is wrong!"
        printf "\n Try again."
        printf "\n"
        exit 1
    fi
    # only options with --start and --end, otherwise fails
    if [[ "$arg" =~ --start ]] ; then
        # with sed
        arg0=$(sed 's/.*--start/--start/' <<< "$arg")
        arg1=$(sed 's/ --start.*//' <<< "$arg")
        arg2=$(sed 's/ --end.*//' <<< "$arg0")
        arg3=$(sed 's/.*--end/--end/' <<< "$arg0")
        # with awk
        # arg0=$(awk -F '--start' '{print FS$2}' <<< "$arg")
        # arg1=$(awk -F ' --start' '{print $1}' <<< "$arg")
        # arg2=$(awk -F ' --end' '{print $1}' <<< "$arg0")
        # arg3=$(awk -F '--end' '{print FS$2}' <<< "$arg0")
        $PLAYER "$arg1" "$arg2" "$arg3"
        retrun
    fi
    # playlist with random items as much as count
    # mpv item1 item2 item3 item4 item5 (can't handle item with --option)
    if [[ -z "$arglist" ]] ; then
        arglist="$arg"
    else
        arglist=""$arglist" "$arg""
    fi
    # test-print arglist "$arglist"
    if (( count < $playlistnumber )) ; then
        return
    fi
    if (( $playlistnumber == 1 )) ; then
        $PLAYER "$arg"
    else
        $PLAYER $arglist
    fi
    count=0
    arglist=""
}

grep-print() {
    if [[ $1 =~ https?:// ]] ; then
        printf "\n"
        awk -F 'http' '{print $1}' <<< $1 | \
            sed 's/^# / /' | grep --color=auto .
        printf "\n"
    elif [[ $1 =~ /$ ]] ; then
        printf "\n"
        sed 's/^# / /' <<< $1 | grep --color=auto .
        printf "\n"
    elif [[ $1 =~ /mnt/ ]] ; then
        printf "\n"
        awk -F '/mnt/' '{print $1}' <<< $1 | \
            sed 's/^# / /' | grep --color=auto .
        printf "\n"
    else
        printf "\n"
        grep --color=auto . <<< $1
        printf "\n"
    fi
}

test-print() {
    # return 0 # to test, comment this line
    printf "\n\n $1 is $2\n\n"
}

remove-parts(){
    if [[ $1 =~ https?:// ]] ; then
        grepnames=$(awk -F 'http' '{print $1}' <<< $1)
    elif [[ $1 =~ /mnt/ ]] ; then
        grepnames=$(awk -F '/mnt/' '{print $1}' <<< $1)
    fi
}

shuffle-radio() {
    local retval shuffleradio
    # eval declare -g $3
    shuffleradio=$(shuf -n 1 <<< $1)
    if [[ -z $2 ]] ; then
        printf "\n start \n"
        retval="$shuffleradio"
    elif [[ $2 == "$shuffleradio" ]] ; then
        printf "\n continue \n"
        continue
    else
        printf "\n normal \n"
        retval="$shuffleradio"
    fi
    eval "$3=\$retval"
}

x=0
while read -r line ; do
    (( x++ ))
    [[ $line =~ ^($RADIOSTARTLINE) ]] && break
done < "$0"
# test-print "RADIOSTARTLINE's number" "$x"
urllist=$(tail -n +"$(( x + 4 ))" "$0")
# test-print urllist "$urllist"

x=0
while read -r line ; do
    (( x++ ))
    [[ $line =~ ^($RADIOENDLINE) ]] && break
done <<< "$urllist"

radiolist=$(head -n +"$(( x - 2 ))" <<< "$urllist")
# test-print radiolist "$radiolist"
urlname=$(awk -F 'http' '{print $1}' <<< "$urllist")
# test-print urlname "$urlname"
radioname=$(awk -F 'http' '{print $1}' <<< "$radiolist")

# only radio
if [[ -z $1 ]] ; then
    while true ; do
        shuffle-radio "$radiolist" "$randomradio" randomradio
        # SKIPPATTERN for comments
        # basic regex: ‘\?’, ‘\+’, ‘\{’, ‘\|’, ‘\(’, and ‘\)’
        (( $(grep -c "$SKIPPATTERN" <<< "$randomradio") )) && continue
        grep-print "$randomradio"
        $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$randomradio") &
        lastbgpid=$!
        wait $lastbgpid
        sleep 1
    done
    exit 0
fi

if [[ $1 =~ ^(-p|--period)$ ]] ; then
    while true ; do
        shuffle-radio "$radiolist" "$randomradio" randomradio
        # SKIPPATTERN for comments
        (( $(grep -c "$SKIPPATTERN" <<< "$randomradio") )) && continue
        printf "\n"
        grep-print "$randomradio"
        $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$randomradio") &
        lastbgpid=$!
        sleep $TESTTIME
        # fail playing
        (( ! $(ps -A | grep -c $lastbgpid) )) && continue
        sleep $PERIODTIME
        kill $lastbgpid
        sleep 1
    done
    exit 0
fi

[[ $1 =~ ^(-a|--all)$ ]] && radio .

if [[ $1 =~ ^(-g|--group)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        case "$2" in
            en) [[ $line =~ ^(## English Radio) ]] && break ;;
            kr) [[ $line =~ ^(## Korean Radio) ]] && break ;;
            jp) [[ $line =~ ^(## Japanese Radio) ]] && break ;;
            mv) [[ $line =~ ^(## Music Video) ]] && break ;;
            lt) [[ $line =~ ^(## Live TV) ]] && break ;;
            ln) [[ $line =~ ^(## Live News) ]] && break ;;
            lc) [[ $line =~ ^(## Live Camera) ]] && break ;;
            pl) [[ $line =~ ^(## Play List) ]] && break ;;
            *) printf "\n Sorry, there is no \"$2\" group."
               printf "\n Try again."
               printf "\n"
               printf "\n Usage: radio -g {en|kr|jp|mv|lt|ln|lc|pl}"
               printf "\n"
               printf "\n en - English Radio"
               printf "\n kr - Korean Radio"
               printf "\n jp - Japanese Radio"
               printf "\n mv - Music Video"
               printf "\n lt - Live TV"
               printf "\n ln - Live News"
               printf "\n lc - Live Camera"
               printf "\n pl - Play List"
               printf "\n"
               exit 1
        esac
    done <<< "$urllist"
    grouplist=$(tail -n +"$(( x + 2 ))" <<< "$urllist")
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^##+ ]] && break
    done <<< "$grouplist"
    grouplist=$(head -n "$(( x - 2 ))" <<< "$grouplist")
    while true ; do
        shuffle-radio "$grouplist" "$randomradio" randomradio
        (( $(grep -c "$SKIPPATTERN" <<< "$randomradio") )) && continue
        grep-print "$randomradio"
        play "$randomradio"
        sleep 1
    done
    exit 0
fi

if [[ $1 =~ ^(-l|--local)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        if [[ $line =~ ^($LOCALSTARTLINE) ]] ; then
            break
        fi
    done <<< "$urllist"
    locallist=$(tail -n +"$(( x + 2 ))" <<< "$urllist")
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^##+ ]] && break
    done <<< "$locallist"
    locallist=$(head -n "$(( x - 2 ))" <<< "$locallist")
    if [[ -z $2 ]] ; then
        while true ; do
            shuffle-radio "$locallist" "$randomlocal" randomlocal
            (( $(grep -c "$SKIPPATTERN" <<< "$randomlocal") )) && continue
            grep-print "$randomlocal"
            randomlocal=$(sed 's/# //' <<< "$randomlocal")
            $PLAYER "$LOCALMUSICDIR""$randomlocal" &
            lastbgpid=$!
            wait $lastbgpid
            sleep 1
        done
    else
        # radio -l "regex"
        cd "$LOCALMUSICDIR"
        shopt -s globstar
        for file in **/* ; do
            if [[ $file =~ $LOCALMUSICEXT ]] ; then
                if [[ -z $localoneline ]] ; then
                    localoneline="$file"
                else
                    localoneline="$localoneline":"$file"
                fi
            fi
        done
        locallist=$(sed s/:/\\n/g <<< $localoneline)
        greplocalcount=$(grep -ic "$2" <<< "$locallist")
        if (( $greplocalcount == 1 )) ; then
            thelocalname=$(grep -i "$2" <<< "$locallist")
            grep-print "$thelocalname"
            $PLAYER "$LOCALMUSICDIR""$thelocalname"
        elif (( $greplocalcount > 1 )) ; then
            localnames=$(grep -i "$2" <<< "$locallist")
            readarray -t arr_localnames <<< "$localnames"
            # Is it possible to select multiple items like 2,4,6 or 3-7?
            select localfile in "${arr_localnames[@]}" ; do
                if [[ $REPLY =~ ^("*"|"00")$ ]] ; then
                    printf "\n"
                    printf "\n You choose all items."
                    printf "\n They'll be played randomly and infinitely."
                    printf "\n To exit, press Ctrl-c repeatedly."
                    printf "\n"
                    while true ; do
                        shuffle-radio "$localnames" "$randomlocal" randomlocal
                        grep-print "$randomlocal"
                        $PLAYER "$LOCALMUSICDIR""$randomlocal" &
                        lastbgpid=$!
                        wait $lastbgpid
                        sleep 1
                    done
                    exit 0
                fi
                selectedlocalfile="$localfile"
                break
            done
            grep-print "$selectedlocalfile"
            $PLAYER "$LOCALMUSICDIR""$selectedlocalfile"
            exit 0
        else
            printf "\n"
            printf "\n Sorry, the \"$2\" does not match."
            printf "\n Try again."
            printf "\n"
            exit 1
        fi
    fi
    exit 0
fi

if [[ $1 =~ ^(-t|--test)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^($TESTENDLINE) ]] && break
    done <<< "$urllist"
    testlist=$(head -n +$x <<< "$urllist")
    cat > "$REPORTPATH" <<< $REPORTFIRSTLINE
    while read -r line ; do
        (( $(grep -c "$SKIPPATTERN" <<< "$line") )) && continue
        grep-print "$line"
        $PLAYER $(awk -F 'http' '{print FS$2}' <<< "$line") &
        lastbgpid=$!
        sleep $TESTTIME
        if (( $(ps aux | grep -c $lastbgpid) == 1 )) ; then
            echo "$line" >> "$REPORTPATH"
        else
            kill $lastbgpid
        fi
        sleep 1
    done <<< "$testlist"
    printf "\n"
    cat "$REPORTPATH"
    printf "\n"
    exit 0
fi

if [[ $1 =~ ^(-e|--edit)$ ]] ; then
    $EDITOR $HOME/git/radio/radio
    exit 0
fi

if [[ $1 =~ ^(-h|--help)$ ]] ; then
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ ^(# Usage) ]] && break
    done < "$0"
    helplist=$(tail -n +$(( x - 3 )) "$0")
    x=0
    while read -r line ; do
        (( x++ ))
        [[ $line =~ (^$) ]] && break
    done <<< "$helplist"
    helplist=$(head -n $x <<< "$helplist")
    printf "\n"
    sed 's/#\+//' <<< "$helplist"
    printf "\n"
    exit 0
fi

# grep 'and' option
for arg in "$@" ; do
    if [[ -z $grepnames ]] ; then
        if [[ $arg =~ ^@[0-9] ]] ; then
            playlistnumber=${arg/@/}
            if (( playlistnumber < 1 )) ; then
                printf "\n"
                printf "\n Sorry, @number is more than 1!"
                printf "\n Try again."
                printf "\n"
                exit 1
            fi
            continue
        fi
        grepnames=$(grep -i "$arg" <<< "$urlname")
    else
        if [[ $arg == -v ]] ; then
            reverse="-v"
            continue
        fi
        grepnames=$(grep -i $reverse "$arg" <<< "$grepnames")
        if [[ -n $reverse ]] ; then
            reverse=""
        fi
    fi
    # test-print playlistnumber "$playlistnumber"
    # test-print grepnames "$grepnames"
done
grepnames=$(sed '/^#\?$\|^##\+\|^#[^ ]\|^#  \+/d' <<< "$grepnames" | \
                sed 's/^# //')
grepcount=$(grep -ic .  <<< "$grepnames")
# test-print grepcount "$grepcount"

if (( $grepcount == 1 )) ; then
    grep-print "$grepnames"
    # grep '-F' for only string like "[live] tv"
    grepline=$(grep -iF "$grepnames" <<< "$urllist")
    play "$grepline"
elif (( $grepcount > 1 )) ; then
    remove-parts "$grepnames"
    readarray -t arr_grepnames <<< "$grepnames"
    select url in "${arr_grepnames[@]}" ; do
        if [[ $REPLY =~ ^("*"|"00")$ ]] ; then
            printf "\n"
            printf "\n You choose all items."
            printf "\n They'll be played randomly and infinitely."
            printf "\n To exit, press Ctrl-c repeatedly"
            printf "\n"
            while true ; do
                shuffle-radio "$grepnames" "$randomurlname" randomurlname
                grep-print "$randomurlname"
                grepline=$(grep -iF "$randomurlname" <<< "$urllist")
                play "$grepline"
                # sleep 1
            done
            exit 0
        fi
        selectedurl="$url"
        break
    done
    grep-print "$selectedurl"
    grepline=$(grep -iF "$selectedurl" <<< "$urllist")
    play "$grepline"
    exit 0
else
    printf "\n"
    printf "\n Sorry, the \"$1\" does not match."
    printf "\n Try again."
    printf "\n"
    exit 1
fi



